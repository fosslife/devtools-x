{
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "Dev Tools",
    "longDescription": "A cross-platform collection of developer friendly utilities",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "licenseFile": "LICENSE",
    "createUpdaterArtifacts": true
  },
  "build": {
    "beforeBundleCommand": "node scripts/compress.mjs",
    "frontendDist": "../dist",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devUrl": "http://localhost:3000"
  },
  "productName": "devtools-x",
  "mainBinaryName": "devtools-x",
  "version": "3.4.0",
  "identifier": "com.fosslife.devtoolsx",
  "plugins": {
    "sql": {
      "preload": [
        "sqlite:devtools.db"
      ]
    },
    "updater": {
      "endpoints": [
        "https://github.com/fosslife/devtools-x/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IERCN0M1MDY1Q0QwRkM1OEUKUldTT3hRL05aVkI4MjhGUW9LSjhZMldoUmNSYmRNYnl0RGNrUW5NTk1lSmNtc2FJTE5NMUhRTzgK"
    }
  },
  "app": {
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": [
          "http://asset.localhost"
        ]
      },
      "dangerousDisableAssetCspModification": [
        "style-src"
      ],
      "csp": {
        "connect-src": [
          "ipc:",
          "http://ipc.localhost",
          "http://asset.localhost",
          "tauri://localhost",
          "asset://localhost"
        ],
        "default-src": [
          "'self'",
          "blob:",
          "data:",
          "filesystem:",
          "ws:",
          "wss:",
          "http:",
          "https:",
          "tauri:",
          "asset:"
        ],
        "worker-src": [
          "'self'",
          "blob:",
          "https://unpkg.com"
        ],
        "script-src": [
          "'self'",
          "'unsafe-inline'"
        ],
        "style-src": [
          "'self'",
          "'unsafe-inline'"
        ]
      }
    },
    "windows": []
  }
}
